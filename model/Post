//Database schema
const mongoose = require('mongoose');
const PostSchema = new mongoose.Schema({

    title: {
        type: String,
        required:false
    },
    author:{
        type: String,
        required:false
    },
    urlstring:{
        type: String,
        required:false
    },
    university:{
        type: String,
        required:false
    },
    datepub:{
        type: String,
        required:false
    }

});

module.exports = mongoose.model('academics',PostSchema);
